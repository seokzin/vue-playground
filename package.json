{
  "name": "vite-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn browser && vite",
    "storybook": "start-storybook -p 6006",
    "build": "run-p type-check vite build",
    "build:storybook": "build-storybook",
    "preview": "vite preview",
    "test": "vitest --environment jsdom --root src/",
    "test:coverage": "yarn test --coverage",
    "test:e2e": "playwright test",
    "type": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "browser": "start http://127.0.0.1:5173",
    "clean": "rm -rf yarn.lock .next node_modules",
    "reinstall": "yarn clean && yarn install",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:css": "stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore",
    "format": "prettier . --ignore-path .gitignore --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@playwright/test": "^1.28.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.7",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.15",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.26.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "babel-loader": "^8.3.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.3",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "storybook-dark-mode": "^2.0.5",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.12"
  }
}
